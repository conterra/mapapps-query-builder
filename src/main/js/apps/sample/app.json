{
    "properties": {
        "title": "${apptitle}"
    },
    "load": {
        "bundleLocations": [
            "localbundles",
            "bundles"
        ],
        "allowedBundles": [
            "system",
            "console",
            "splashscreen",
            "templatelayout",
            "template-seasons",
            "theme-autumn",
            "toolset",
            "map-init",
            "map-preload-2D",
            "map-preload-3D",
            "mapnavigation",
            "notifier",
            "agssearch",
            "banner",
            "wizard",
            "authentication",
            "viewmodeswitcher",
            "popups-default",
            "toc",
            "selection-ui",
            "dn_querybuilder"
        ],
        "require": [
            "${app}.app"
        ],
        "i18n": [
            "bundle"
        ]
    },
    "bundles": {
        "dn_querybuilder": {
            "QueryBuilderProperties": {
                "enableDistinctValues": true,
                "enableInitialDistinctValues": true,
                "defaultLinkOperator": "$or",
                "defaultSpatialRelation": "everywhere",
                "useUserExtent": false,
                "allowNegation": true,
                "showQuerySettingsInEditableMode": true,
                "showSortSelectInUserMode": false,
                "showFieldType": true,
                "showSpatialRelation": true,
                "showSpatialInputActions": false,
                "hiddenFields": [
                    "objectid",
                    "OBJECTID",
                    "shape"
                ],
                "hiddenSortFields": [
                    "objectid",
                    "OBJECTID",
                    "shape"
                ]
            },
            "QueryBuilderWidgetModel": {
                "storeIds": [
                    "standorte",
                    "countries",
                    "baumkataster",
                    "stoerungen",
                    "wahlkreise_strukturdaten",
                    "gemeinden"
                ]
            },
            "QueryTools": [
                {
                    "id": "querybuilder_1441021732534",
                    "title": "${queries.q_street_damage}",
                    "iconClass": "icon-toolcase",
                    "storeId": "stoerungen",
                    "complexQuery": {
                        "geometry": {
                            "$contains": {
                                "xmin": -1454749.7653909875,
                                "ymin": 4027109.2609186354,
                                "xmax": 4454749.7653909875,
                                "ymax": 8972890.739081364,
                                "spatialReference": {
                                    "wkid": 3857
                                }
                            }
                        },
                        "$and": [
                            {
                                "art": {
                                    "$eq": 5
                                }
                            },
                            {
                                "status": {
                                    "$eq": 1
                                }
                            }
                        ]
                    },
                    "options": {
                        "editable": false,
                        "mode": "builder",
                        "count": -1,
                        "ignoreCase": false,
                        "locale": {
                            "language": "en",
                            "country": "EN"
                        },
                        "sort": [
                            {
                                "attribute": "details",
                                "descending": true
                            }
                        ]
                    }
                },
                {
                    "id": "querybuilder_1540467316530",
                    "title": "${queries.q_constituency_density_1000}",
                    "iconClass": "icon-custom-info",
                    "complexQuery": {
                        "$or": [
                            {
                                "Bevölkerungsdichte": {
                                    "$gt": 1000
                                }
                            }
                        ]
                    },
                    "options": {
                        "mode": "builder",
                        "editable": false,
                        "count": -1,
                        "ignoreCase": false,
                        "locale": {
                            "language": "en",
                            "country": "EN"
                        }
                    },
                    "storeId": "wahlkreise_strukturdaten"
                },
                {
                    "id": "querybuilder_1540467904227",
                    "title": "${queries.q_constituency_income_20000}",
                    "iconClass": "icon-custom-info",
                    "complexQuery": {
                        "$or": [
                            {
                                "Haushaltseinkommen_eur_je_Einw": {
                                    "$gte": 20000
                                }
                            }
                        ]
                    },
                    "options": {
                        "mode": "builder",
                        "editable": false,
                        "count": -1,
                        "ignoreCase": false,
                        "locale": {
                            "language": "en",
                            "country": "EN"
                        }
                    },
                    "storeId": "wahlkreise_strukturdaten"
                },
                {
                    "id": "querybuilder_1540468225267",
                    "title": "${queries.q_constituency_inhabitants_increasing}",
                    "iconClass": "icon-custom-info",
                    "complexQuery": {
                        "$or": [
                            {
                                "Geburtensaldo_je1000": {
                                    "$gt": 0
                                }
                            }
                        ]
                    },
                    "options": {
                        "mode": "builder",
                        "editable": false,
                        "count": -1,
                        "ignoreCase": false,
                        "locale": {
                            "language": "en",
                            "country": "EN"
                        }
                    },
                    "storeId": "wahlkreise_strukturdaten"
                },
                {
                    "id": "querybuilder_1550577927595",
                    "title": "${queries.q_find_tree_species}",
                    "iconClass": "icon-custom-info",
                    "complexQuery": {
                        "$or": [
                            {
                                "gattung": {
                                    "$eq": "Ahorn (Acer)"
                                }
                            }
                        ]
                    },
                    "options": {
                        "mode": "builder",
                        "editable": true,
                        "editOptions": {
                            "spatialRelation": true,
                            "linkOperator": true,
                            "editFields": [
                                {
                                    "not": false,
                                    "field": false,
                                    "relationalOperator": false,
                                    "value": true
                                }
                            ]
                        },
                        "count": -1,
                        "ignoreCase": false,
                        "locale": {
                            "language": "en",
                            "country": "EN"
                        }
                    },
                    "storeId": "baumkataster"
                },
                {
                    "id": "querybuilder_1572005863861",
                    "title": "${queries.q_find_disturbances}",
                    "iconClass": "icon-custom-info",
                    "complexQuery": {
                        "$or": [
                            {
                                "zeitpunkt": {
                                    "$lte": "2019-10-25T12:18:15.749Z"
                                }
                            }
                        ]
                    },
                    "options": {
                        "mode": "builder",
                        "editable": true,
                        "editOptions": {
                            "spatialRelation": true,
                            "linkOperator": true,
                            "editFields": [
                                {
                                    "not": false,
                                    "field": false,
                                    "relationalOperator": false,
                                    "value": true
                                }
                            ]
                        },
                        "count": -1,
                        "ignoreCase": false,
                        "locale": {
                            "language": "en",
                            "country": "EN"
                        }
                    },
                    "storeId": "stoerungen"
                }
            ]
        },
        "banner": {
            "BannerWidget": {
                "label": "Developer Network",
                "imagePosition": "above",
                "image": "resource('${app}:/images/logo_conterra.png')",
                "imageWidth": 200,
                "link": "https://www.conterra.de"
            }
        },
        "resultcenter": {
            "ActionController": {
                "zoomToFeatures": true,
                "zoomToSelected": false,
                "centerSelected": true,
                "zoomAfterDeletion": false,
                "zoomToClicked": false,
                "centerClicked": false,
                "openPopup": true
            },
            "FeatureMapVisualizer": {
                "useDataViewPaging": true
            },
            "DataView": {
                "DGRID": {
                    "checkboxSelection": true,
                    "columns": [
                        {
                            "matches": {
                                "name": {
                                    "$exists": true
                                }
                            },
                            "width": 150
                        }
                    ]
                }
            }
        },
        "agssearch": {
            "AutoStoreRegistration": {
                "componentEnabled": true
            },
            "AGSStore": [
                {
                    "id": "countries",
                    "title": "Countries",
                    "description": "Countries of the world",
                    "url": "https://services.conterra.de/arcgis/rest/services/common/grenzen/MapServer/3",
                    "popupEnabled": true,
                    "useIn": [
                        "querybuilder"
                    ],
                    "filterOptions": {
                        "metadata": {
                            "fields": [
                                {
                                    "name": "objectid",
                                    "title": "ID"
                                },
                                {
                                    "name": "placename",
                                    "title": "Name"
                                },
                                {
                                    "name": "code",
                                    "title": "Code"
                                }
                            ]
                        }
                    }
                },
                {
                    "id": "baumkataster",
                    "title": "Baumkataster",
                    "description": "Baumkataster",
                    "url": "https://services.conterra.de/arcgis/rest/services/mapapps/Baumkataster_Zustand/FeatureServer/0",
                    "popupEnabled": true,
                    "useIn": [
                        "querybuilder"
                    ],
                    "filterOptions": {
                        "metadata": {
                            "fields": [
                                {
                                    "name": "objectid",
                                    "title": "Object ID"
                                },
                                {
                                    "name": "id",
                                    "title": "ID"
                                },
                                {
                                    "name": "anlage",
                                    "title": "Anlage"
                                },
                                {
                                    "name": "gattung",
                                    "title": "Gattung"
                                },
                                {
                                    "name": "pflanzjahr",
                                    "title": "Pflanzjahr"
                                },
                                {
                                    "name": "status",
                                    "title": "Status"
                                },
                                {
                                    "name": "hoehe",
                                    "title": "Höhe"
                                },
                                {
                                    "name": "stammdurchmesser",
                                    "title": "Stammdurchmesser"
                                }
                            ]
                        }
                    }
                },
                {
                    "id": "stoerungen",
                    "title": "Störungen",
                    "description": "Störungen",
                    "idProperty": "objectid",
                    "url": "https://services.conterra.de/arcgis/rest/services/training/stoerungen/MapServer/0",
                    "popupEnabled": true,
                    "useIn": [
                        "querybuilder"
                    ]
                },
                {
                    "id": "wahlkreise_strukturdaten",
                    "title": "Wahlkreise 2017 - Strukturdaten",
                    "description": "Wahlkreise 2017 - Strukturdaten",
                    "idProperty": "OBJECTID",
                    "url": "https://services2.arcgis.com/jUpNdisbWqRpMo35/ArcGIS/rest/services/Wahlkreise_2017_mit_Strukturdaten/FeatureServer/0",
                    "popupEnabled": true,
                    "useIn": [
                        "querybuilder"
                    ]
                },
                {
                    "id": "gemeinden",
                    "title": "Gemeinden",
                    "description": "Gemeinden",
                    "idProperty": "objectid",
                    "url": "https://services.conterra.de/arcgis/rest/services/common/grenzen/FeatureServer/0",
                    "popupEnabled": true,
                    "useIn": [
                        "querybuilder"
                    ]
                }
            ]
        },
        "coordinateviewer": {
            "Config": {
                "decimalPlaces": 3,
                "showScale": true,
                "showCoordinates": true,
                "degreeFormat": true,
                "transformSrs": true,
                "targetSrs": "WGS84"
            }
        },
        "toolset": {
            "ToolsetManager": {
                "toolsets": [
                    {
                        "id": "mapview_tools",
                        "tools": [
                            "zoomInTool",
                            "zoomOutTool",
                            "compassTool",
                            "restoreInitialViewTool",
                            "viewmodeSwitcherTool"
                        ],
                        "registerWidget": {
                            "widgetRole": "mapview_tools"
                        },
                        "container": "ignore",
                        "windowType": "container",
                        "cssClass": "muted"
                    },
                    {
                        "id": "questions",
                        "title": "${toolset.toolset_title}",
                        "container": "map",
                        "window": {
                            "closable": true
                        },
                        "tooltip": "${toolset.toolset_hover}",
                        "cssClass": "ctWDYWBtn ctPrimaryInput",
                        "max_horizontal": 1,
                        "windowType": "dropdown",
                        "position": {
                            "rel_l": 20,
                            "rel_t": 20
                        },
                        "tools": [
                            "querybuilder_*",
                            "queryBuilderToggleTool",
                            "selection-ui-tool"
                        ]
                    }
                ]
            }
        },
        "toc": {
            "Config": {
                "actions": [
                    "show-description",
                    "zoom-to-extent",
                    "activate-children",
                    "deactivate-children",
                    "change-opacity",
                    "show-copyright",
                    "querybuilder-filter"
                ]
            }
        },
        "map-init": {
            "Config": {
                "basemaps": [
                    {
                        "id": "esri_street",
                        "title": "Straßenkarte (grau)",
                        "thumbnailUrl": "resource('${app}:/images/streets.png')",
                        "selected": true,
                        "basemap": "gray-vector"
                    },
                    {
                        "id": "esri_hybrid",
                        "title": "Luftbild (hybrid)",
                        "thumbnailUrl": "resource('${app}:/images/hybrid.png')",
                        "basemap": "hybrid"
                    }
                ],
                "map": {
                    "layers": [
                        {
                            "id": "koeln",
                            "url": "https://services.conterra.de/arcgis/rest/services/common/koeln/MapServer",
                            "type": "AGS_DYNAMIC"
                        },
                        {
                            "id": "standorte",
                            "url": "https://services.conterra.de/arcgis/rest/services/common/standorte/FeatureServer/0",
                            "type": "AGS_FEATURE"
                        },
                        {
                            "id": "earthquakes",
                            "url": "http://sampleserver6.arcgisonline.com/arcgis/rest/services/Earthquakes_Since1970/MapServer",
                            "type": "AGS_DYNAMIC",
                            "expanded": true
                        }
                    ],
                    "ground": {
                        "url": "https://elevation3d.arcgis.com/arcgis/rest/services/WorldElevation3D/Terrain3D/ImageServer",
                        "type": "ELEVATION"
                    }
                },
                "view": {
                    "viewmode": "2D",
                    "extent": {
                        "xmin": -1000000,
                        "ymin": 3000000,
                        "xmax": 4000000,
                        "ymax": 10000000,
                        "spatialReference": 3857
                    }
                }
            }
        },
        "templates": {
            "TemplateModel": {
                "_selectedTemplate": "seasons",
                "widgets": [
                    {
                        "widgetRole": "queryBuilderWidget",
                        "sublayout": [
                            "desktop"
                        ],
                        "window": {
                            "marginBox": {
                                "w": 600,
                                "h": 400,
                                "l": 20,
                                "t": 120
                            }
                        }
                    },
                    {
                        "widgetRole": "editableQueryBuilderWidget",
                        "sublayout": [
                            "desktop"
                        ],
                        "window": {
                            "marginBox": {
                                "w": 600,
                                "h": 400,
                                "l": 20,
                                "t": 120
                            }
                        }
                    },
                    {
                        "widgetRole": "filterQueryBuilderWidget",
                        "sublayout": [
                            "desktop"
                        ],
                        "window": {
                            "marginBox": {
                                "w": 600,
                                "h": 400,
                                "r": 20,
                                "t": 120
                            }
                        }
                    }
                ]
            }
        },
        "themes": {
            "ThemeModel": {
                "_selectedTheme": "everlasting"
            }
        }
    }
}
